#include<stdio.h>

int Factorial(int iNo)
{
	int i = 1, Ans = 1; 
	
	for(i=1; i<=iNo; i++)
	{ 
		Ans = Ans * i;
	}
	
	return Ans;
	
}

int main()
{
	int iRet = 0;
	int iValue = 0;
	
	printf("Enter the value : ");
	scanf("%d",&iValue);
	
	iRet = Factorial(iValue);
	printf("Factorial is : %d", iRet);
	
	return 0;
}